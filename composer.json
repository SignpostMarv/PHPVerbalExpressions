{
    "name": "verbalexpressions/php-verbal-expressions",
    "license": "MIT",
    "type": "project",
    "description": "PHP Regular expressions made easy",
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11",
        "phpunit/phpunit": "* >=4",
        "signpostmarv/verbal-expressions-tests": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/SignpostMarv/Verbal-Expressions-Tests.git"
        }
    ],
    "suggest": {
        "fabpot/php-cs-fixer": "PHP CS Fixer (testing for PSR-2 compliance)"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "VerbalExpressions\\PHPVerbalExpressions\\": "src/"
        }
    }
}
